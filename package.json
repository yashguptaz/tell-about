{
  "name": "tell-about",
  "version": "1.2.5",
  "description": "A CLI Tool that help you know about npm pacakges right in the command line.",
  "main": "dist/index.js",
  "bin": {
    "tell-about": "dist/index.js"
  },
  "scripts": {
    "commit": "git-cz",
    "prebuild": "rimraf dist",
    "build": "babel --copy-files --out-dir dist --ignore *.test.js src",
    "test": "nyc mocha src/index.test.js",
    "watch:test": "mocha --watch src/index.test.js",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "lint": "eslint .",
    "validate": "npm-run-all --parallel lint test build",
    "semantic-release": "semantic-release",
    "semver": "semver",
    "release": "semantic-release pre && npm publish && semantic-release post"
  },
  "keywords": [],
  "files": [
    "dist"
  ],
  "author": "Yash Gupta <yashguptaofficial@gmail.com> (https://yashguptaz.com/)",
  "license": "MIT",
  "dependencies": {
    "chalk": "4.0.0",
    "enquirer": "2.3.5",
    "node-fetch": "2.6.0",
    "regenerator-runtime": "0.13.5"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-preset-env": "1.7.0",
    "chai": "4.2.0",
    "codecov": "3.7.1",
    "commitizen": "4.0.5",
    "cz-conventional-changelog": "3.1.0",
    "eslint": "6.8.0",
    "ghooks": "2.0.4",
    "mocha": "7.1.2",
    "npm-run-all": "4.1.5",
    "nyc": "15.0.1",
    "semantic-release": "^17.0.7",
    "semver": "7.3.2",
    "validate-commit-msg": "2.14.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npm run validate",
      "commit-msg": "validate-commit-msg"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yashguptaz/tell-about.git"
  }
}
